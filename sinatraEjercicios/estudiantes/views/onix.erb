<h1>Hola Onix</h1>

<ul>
  <% @students.each do |name| %>
    <li><%= name %></li>
  <% end %>
</ul>


<h1>Nuevo estudiante</h1>

<form action="/onix/students/create" method="POST">
  <label for="nickname">Nickname</label>
  <input type="text" name="nickname">
  <input type="submit" value="Crea estudiante">
</form>
